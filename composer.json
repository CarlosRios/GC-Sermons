{
	"name": "gc-sermons",
	"description": "Manage sermons and sermon content in WordPress",
	"keywords": ["wordpress", "plugin", "generator-plugin-wp"],
	"homepage": "http://dsgnwrks.pro",
	"license": "GPLv2",
	"authors": [
	{
		"name": "jtsternberg",
		"homepage": "http://dsgnwrks.pro"
	}
	],
	"minimum-stability": "dev",
	"type": "wordpress-plugin",
	"autoload": {
		"classmap": ["gc-sermons.php", "includes/", "vendor/webdevstudios/cpt-core/CPT_Core.php", "vendor/webdevstudios/taxonomy_core/Taxonomy_Core.php"]
	},
	"require": {
		"php": ">=5.2",
		"xrstf/composer-php52": "1.*",
		"webdevstudios/cpt-core": "dev-master",
		"webdevstudios/taxonomy_core": "^0.2.4",
		"webdevstudios/cmb2-user-select": "^0.1.0"
	},
	"scripts": {
        "post-install-cmd": [
            "xrstf\\Composer52\\Generator::onPostInstallCmd"
        ],
        "post-update-cmd": [
            "xrstf\\Composer52\\Generator::onPostInstallCmd"
        ],
        "post-autoload-dump": [
            "xrstf\\Composer52\\Generator::onPostInstallCmd"
        ]
	}
}
